<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/lightBlack"    tools:context=".MainActivity">    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtLblTitle"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_margin="@dimen/_10sdp"        android:fontFamily="@font/poppins_bold"        android:text="BMI Calculator"        android:textColor="@android:color/white"        android:textSize="@dimen/_20sdp"        app:fontFamily="@font/poppins_bold"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <androidx.appcompat.widget.AppCompatImageView        android:id="@+id/btnReset"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="@dimen/_10sdp"        android:background="@drawable/ornagecolor_corner"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/ic_reset" />    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clGenderSelect"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/_10sdp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/btnReset">        <androidx.appcompat.widget.AppCompatButton            android:id="@+id/btnMale"            android:layout_width="@dimen/_140sdp"            android:layout_height="@dimen/_80sdp"            android:background="@drawable/lightblackcorner_corner"            android:drawableTop="@drawable/ic_men"            android:fontFamily="@font/poppins_bold"            android:text="Male"            android:textColor="@android:color/white"            android:textSize="@dimen/_15sdp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/btnFemale"            app:layout_constraintHorizontal_bias="0.5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <androidx.appcompat.widget.AppCompatButton            android:id="@+id/btnFemale"            android:layout_width="@dimen/_140sdp"            android:layout_height="@dimen/_80sdp"            android:background="@drawable/graycolor_corner"            android:drawableTop="@drawable/ic_woman"            android:fontFamily="@font/poppins_bold"            android:text="Female"            android:textColor="@android:color/white"            android:textSize="@dimen/_15sdp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.5"            app:layout_constraintStart_toEndOf="@+id/btnMale"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clInputFromUser"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/_10sdp"        android:padding="@dimen/_15sdp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/clGenderSelect">        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtLblHeight"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="@dimen/_20sdp"            android:layout_marginStart="@dimen/_20sdp"            android:fontFamily="@font/poppins_medium"            android:text="Height"            android:textColor="@color/grayColor"            android:textSize="@dimen/_17sdp"            app:layout_constraintBottom_toBottomOf="@id/editHeight"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@id/editHeight" />        <androidx.appcompat.widget.AppCompatEditText            android:id="@+id/editHeight"            android:layout_width="@dimen/_150sdp"            android:layout_height="wrap_content"            android:layout_margin="@dimen/_20sdp"            android:layout_marginEnd="@dimen/_20sdp"            android:background="@drawable/graycolor_corner"            android:fontFamily="@font/poppins_medium"            android:hint="Enter Height in CM"            android:inputType="number"            android:singleLine="true"            android:textColor="@android:color/white"            android:textColorHint="@android:color/white"            android:textSize="@dimen/_14sdp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtLblWeight"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="@dimen/_20sdp"            android:layout_marginStart="@dimen/_20sdp"            android:fontFamily="@font/poppins_medium"            android:text="Weight"            android:textColor="@color/grayColor"            android:textSize="@dimen/_17sdp"            app:layout_constraintBottom_toBottomOf="@id/editWeight"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@id/editWeight" />        <androidx.appcompat.widget.AppCompatEditText            android:id="@+id/editWeight"            android:layout_width="@dimen/_150sdp"            android:layout_height="wrap_content"            android:layout_margin="@dimen/_20sdp"            android:layout_marginEnd="@dimen/_20sdp"            android:background="@drawable/graycolor_corner"            android:fontFamily="@font/poppins_medium"            android:hint="Enter Weight in KG"            android:inputType="number"            android:singleLine="true"            android:textColor="@android:color/white"            android:textColorHint="@android:color/white"            android:textSize="@dimen/_14sdp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toBottomOf="@id/editHeight" />        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtLblAge"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="@dimen/_20sdp"            android:layout_marginStart="@dimen/_20sdp"            android:fontFamily="@font/poppins_medium"            android:text="Age"            android:textColor="@color/grayColor"            android:textSize="@dimen/_17sdp"            app:layout_constraintBottom_toBottomOf="@id/editAge"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@id/editAge" />        <androidx.appcompat.widget.AppCompatEditText            android:id="@+id/editAge"            android:layout_width="@dimen/_150sdp"            android:layout_height="wrap_content"            android:layout_margin="@dimen/_20sdp"            android:layout_marginEnd="@dimen/_20sdp"            android:background="@drawable/graycolor_corner"            android:fontFamily="@font/poppins_medium"            android:hint="Enter your age"            android:inputType="number"            android:singleLine="true"            android:textColor="@android:color/white"            android:textColorHint="@android:color/white"            android:textSize="@dimen/_14sdp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toBottomOf="@id/editWeight" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.appcompat.widget.AppCompatButton        android:id="@+id/btnCalculate"        android:layout_width="0dp"        android:layout_height="@dimen/_45sdp"        android:layout_marginStart="@dimen/_30sdp"        android:layout_marginTop="@dimen/_20sdp"        android:layout_marginEnd="@dimen/_30sdp"        android:background="@drawable/bluecolor_corner"        android:fontFamily="@font/poppins_semibold"        android:text="Calculate Your BMI"        android:textAllCaps="false"        android:textColor="@android:color/white"        android:textSize="@dimen/_20sdp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/clInputFromUser" /></androidx.constraintlayout.widget.ConstraintLayout>